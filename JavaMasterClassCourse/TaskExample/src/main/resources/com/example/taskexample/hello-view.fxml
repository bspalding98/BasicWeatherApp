<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>

<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<GridPane xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.example.taskexample.HelloController" alignment="center" hgap="10" vgap="10">
    <Button text="Get Employee Names" onAction="#buttonPressed"
            GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <ListView fx:id="listView" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
    <ProgressBar fx:id="progressBar" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <Label fx:id="progressLabel" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
</GridPane>
